{
  "expo": {
    "name": "submarkettest",
    "slug": "submarkettest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon10.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "NOT_REQUIRED"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber" : "1.0.0",
      "bundleIdentifier": "com.submarket.submarkettest",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511"
      }
    },
    "android": {
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2052781801234117~5766333626"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon10.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.leeminki.submarkettest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
